{
  "name": "agra-ranker",
  "description": "Agra web server to rank greyhounds",
  "version": "6.1.0",
  "private": false,
  "license": "MIT",
  "author": "Scott Brown",
  "repository": {
    "type": "git",
    "url": "https://github.com/link1900/agra-ranker.git"
  },
  "engines": {
    "node": "7.4.0",
    "npm": "4.0.5"
  },
  "scripts": {
    "start": "node node_modules/gulp/bin/gulp",
    "dev": "npm start",
    "lint": "node ./node_modules/eslint/bin/eslint.js ./app ./client",
    "spec": "./node_modules/.bin/istanbul cover _mocha ./specs/*.js ./app/**/*Test.js",
    "test": "npm run lint && npm run mocha",
    "mocha": "mocha ./specs/*.js ./app/**/*Test.js",
    "postinstall": "node node_modules/bower/bin/bower install && node node_modules/gulp/bin/gulp js"
  },
  "dependencies": {
    "JSONStream": "1.0.4",
    "bower": "1.3.12",
    "browser-sync": "2.17.3",
    "csv": "1.1.1",
    "dotenv": "4.0.0",
    "express": "3.4.8",
    "express-jwt": "5.1.0",
    "gulp": "3.9.1",
    "gulp-angular-templatecache": "1.9.1",
    "gulp-concat": "2.6.0",
    "gulp-nodemon": "2.2.1",
    "highland": "2.10.0",
    "jsonwebtoken": "7.1.9",
    "lodash": "4.16.4",
    "moment-timezone": "0.5.6",
    "mongoose": "4.7.9",
    "mongoose-concrete-timestamps": "0.1.0",
    "q": "1.4.1",
    "request": "2.75.0",
    "winston": "0.8.3",
    "x-ray": "2.3.1",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.0",
    "istanbul": "0.4.5",
    "mocha": "3.4.2",
    "supertest": "0.10.0"
  }
}
